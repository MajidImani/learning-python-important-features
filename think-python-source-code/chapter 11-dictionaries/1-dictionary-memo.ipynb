   "source": [
    "known = {0:0 , 1:1}\n",
    "def fibonacci(n):\n",
    "    if n in known:\n",
    "        return known[n]\n",
    "    \n",
    "    result = fibonacci(n-1) + fibonacci(n-2)\n",
    "    known[n] = result\n",
    "    return result"
   ]
